<view class="data-v-c10d0c50"><block wx:if="{{addressShow}}"><view class="go_address data-v-c10d0c50"><view class="address_btn data-v-c10d0c50"><button type="primary" plain="{{true}}" class="data-v-c10d0c50">获取收货地址</button></view></view></block><block wx:else><view class="new_address data-v-c10d0c50"><view class="addreperson data-v-c10d0c50"><text class="data-v-c10d0c50">{{"收货人:"+address.userName}}</text><text class="data-v-c10d0c50">{{address.telNumber}}</text></view><view class="address_bottom data-v-c10d0c50">{{''+(address.provinceName+address.cityName+address.countyName+address.detailInfo)+''}}</view></view></block><block wx:for="{{cartList}}" wx:for-item="item" wx:for-index="index" wx:key="goods_id"><view class="cart_contents data-v-c10d0c50"><view class="cart_item data-v-c10d0c50"><view class="cart_info data-v-c10d0c50"><navigator class="data-v-c10d0c50"><image src="{{item.goods_small_logo}}" mode="widthFix" class="data-v-c10d0c50"></image></navigator><view class="cart_info_list data-v-c10d0c50"><text class="data-v-c10d0c50">{{item.goods_name}}</text><view class="cart_edit data-v-c10d0c50"><view class="goods_price data-v-c10d0c50">{{''+item.goods_price+''}}</view></view></view></view></view></view></block><view class="foot_tool data-v-c10d0c50"><view class="total_price_warp data-v-c10d0c50"><view class="total_top data-v-c10d0c50">合计：<text class="num_price data-v-c10d0c50">{{totalPrice}}</text></view><view class="yunfei data-v-c10d0c50">包含运费</view></view><view class="order_pay_warp data-v-c10d0c50">{{'结算'+totalNum+''}}</view></view></view>