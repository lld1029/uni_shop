page {
  height: 100%;
}
.cates {
  height: 100%;
}
.cates_contents {
  width: 100vw;
  height: calc(100vh - (44px + 45px));
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.cates_contents .left_menu {
  -webkit-box-flex: 2;
  -webkit-flex: 2;
          flex: 2;
}
.cates_contents .left_menu .left_list {
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  position: relative;
}
.cates_contents .left_menu .active {
  color: red;
  background: #C0C0C0;
}
.cates_contents .right_menu {
  -webkit-box-flex: 5;
  -webkit-flex: 5;
          flex: 5;
}
.cates_contents .right_menu .right_list .titles {
  width: 100%;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  color: #C0C0C0;
  letter-spacing: 5px;
}
.cates_contents .right_menu .right_list .goodsinfo {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  text-align: center;
}
.cates_contents .right_menu .right_list .goodsinfo navigator {
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  width: 33.33%;
  padding: 10px;
}
.cates_contents .right_menu .right_list .goodsinfo navigator image {
  width: 100%;
}

